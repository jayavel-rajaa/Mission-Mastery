#include<iostream>
#include<cmath>
using namespace std;
int main(){
  int n;
  float avg=0;
  cin>>n;
  if(n<0){
    cout<<"Invalid Input";
    return 0;
  }
  int arr[n];
  for(int i=0;i<n;i++){
    cin>>arr[i];
    if(arr[i]>=0){
      avg=avg+arr[i];
    }
    else{
      cout<<"Invalid Input";
      return 0;
  }
  
}avg=avg/n;
  cout<<avg
}
