#include<iostream>
using namespace std;
int main(){
  int r,c,max=0;
  cin>>r;
  cin>>c;
  int arr[r][c];
  for(int i=0;i<r;i++){
    for(int j=0;j<c;j++){
      cin>>arr[i][j];
    }
  }
for(int i=0;i<r;i++){

  for(int j=0;j<c;j++){
      max = arr[0][0];
      if(arr[i][j]>max)
      max = arr[i][j];
  }
  cout<<max<<" ";
}
}
